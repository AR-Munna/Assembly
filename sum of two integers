segment .data
fmt: db "%d %d", 0
fmt2: db "%d %d", 10, 0
fmt3: db "%d", 10, 0

segment .bss
hello: resq 1
h2: resq 1
sum: resq 1

segment .text

global main
extern scanf
extern printf

main:
push rbp

mov rdi, fmt
mov rsi, hello
mov rdx, h2
mov rax, 0
call scanf



mov rdi, fmt3
mov rcx, 0
add rcx, [hello]
add rcx, [h2]
mov rsi, rcx
mov rax, 0
call printf

;add rdx, '4'
;mov rdx, 0
;call printf

mov rax, 0
pop rbp
ret

